    "react-native": "0.81.4",
    "react-native-bluetooth-classic": "^1.73.0-rc.15",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
